container_commands:
    # Copy env configuration file over
    01_config_environment:
        command: mv /var/app/current/.env.example /var/app/current/.env
    # Set up Laravel key
    02_set_application_key:
        command: php artisan key:generate
        cwd: /var/app/current
    # Migrate db
    03_migrate_db:
        command: php artisan migrate --force
        cwd: /var/app/current